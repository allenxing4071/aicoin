# AIcoin 环境变量配置模板
# 复制此文件为 .env 并填入真实的密钥

# ============================================
# 数据库配置
# ============================================
DATABASE_URL=postgresql://aicoin:your_password@localhost:5433/aicoin
REDIS_URL=redis://localhost:6379

# ============================================
# AI 平台 API 密钥
# ============================================

# DeepSeek
DEEPSEEK_API_KEY=your_deepseek_api_key_here

# Qwen (阿里云)
QWEN_API_KEY=your_qwen_api_key_here

# Claude (Anthropic)
CLAUDE_API_KEY=your_claude_api_key_here

# OpenAI
OPENAI_API_KEY=your_openai_api_key_here

# Grok
GROK_API_KEY=your_grok_api_key_here

# ============================================
# 交易所 API 密钥
# ============================================

# Hyperliquid
HYPERLIQUID_WALLET_ADDRESS=your_wallet_address
HYPERLIQUID_PRIVATE_KEY=your_private_key
HYPERLIQUID_VAULT_ADDRESS=your_vault_address
HYPERLIQUID_TESTNET=true

# Binance
BINANCE_API_KEY=your_binance_api_key
BINANCE_API_SECRET=your_binance_api_secret
BINANCE_TESTNET=false

# ============================================
# 情报平台 API 密钥
# ============================================

# 腾讯云（用于新闻采集等）
TENCENT_SECRET_ID=your_tencent_secret_id
TENCENT_SECRET_KEY=your_tencent_secret_key

# 百度千帆
BAIDU_API_KEY=your_baidu_api_key
BAIDU_SECRET_KEY=your_baidu_secret_key

# AWS
AWS_ACCESS_KEY=your_aws_access_key
AWS_SECRET_KEY=your_aws_secret_key

# ============================================
# 应用配置
# ============================================

# JWT 密钥（用于用户认证）
JWT_SECRET_KEY=your-super-secret-jwt-key-change-in-production

# 应用密钥
SECRET_KEY=your-super-secret-app-key-change-in-production

# 调试模式
DEBUG=true

# ============================================
# 交易配置
# ============================================

# 交易开关
TRADING_ENABLED=false

# 活跃交易所
ACTIVE_EXCHANGE=hyperliquid

# 活跃市场类型
ACTIVE_MARKET_TYPE=perpetual

# AI 决策间隔（秒）
DECISION_INTERVAL=600

# ============================================
# 向量数据库配置
# ============================================
QDRANT_HOST=localhost
QDRANT_PORT=6333
QDRANT_COLLECTION_NAME=trading_memories

# ============================================
# 注意事项
# ============================================
# 1. 不要将 .env 文件提交到 Git
# 2. 所有密钥都应该保密
# 3. 生产环境必须使用强密钥
# 4. 定期轮换密钥
# 5. 不要在文档中包含真实密钥
