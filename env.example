# Database
DATABASE_URL=postgresql://admin:your_password@localhost:5433/aicoin
DB_PASSWORD=your_password

# Redis
REDIS_URL=redis://localhost:6379

# LLM API
DEEPSEEK_API_KEY=sk-your-deepseek-api-key
QWEN_API_KEY=sk-your-qwen-api-key
GROK_API_KEY=xai-your-grok-api-key
CLAUDE_API_KEY=sk-ant-your-claude-api-key
OPENAI_API_KEY=sk-your-openai-api-key

# Hyperliquid
HYPERLIQUID_WALLET_ADDRESS=0x...
HYPERLIQUID_PRIVATE_KEY=0x...
HYPERLIQUID_TESTNET=true

# Binance
BINANCE_API_KEY=your-binance-api-key
BINANCE_API_SECRET=your-binance-api-secret
BINANCE_TESTNET=false
BINANCE_BASE_URL=https://api.binance.com
BINANCE_FUTURES_BASE_URL=https://fapi.binance.com

# Security
SECRET_KEY=your-secret-key-change-in-production
JWT_SECRET_KEY=your-jwt-secret-key
ALGORITHM=HS256
ACCESS_TOKEN_EXPIRE_MINUTES=30

# Trading Config
DEFAULT_SYMBOL=BTC-PERP
TRADING_ENABLED=false
DECISION_INTERVAL=300
MAX_POSITION_PCT=0.20
MAX_DAILY_LOSS_PCT=0.05
MAX_DRAWDOWN_PCT=0.10

# ===== Intelligence System Configuration (情报系统配置) =====

# 多平台协调配置
INTELLIGENCE_USE_MULTI_PLATFORM=true  # 启用多平台协调
INTELLIGENCE_USE_STORAGE_LAYERS=true  # 启用四层存储
INTELLIGENCE_UPDATE_INTERVAL=1800  # 情报更新间隔（秒，默认30分钟）

# 云平台并行验证配置
CLOUD_PLATFORM_PARALLEL_ENABLED=true  # 启用云平台并行验证
CLOUD_PLATFORM_MIN_CONSENSUS=0.6  # 最小共识度阈值（60%）

# 存储层配置
L1_CACHE_TTL_HOURS=24  # L1缓存过期时间（小时）
L2_ANALYSIS_INTERVAL_HOURS=1  # L2分析间隔（小时）
L4_VECTOR_DIMENSION=1536  # L4向量维度

# Qdrant Vector Database
QDRANT_HOST=localhost
QDRANT_PORT=6333
QDRANT_COLLECTION_NAME=trading_memories

# 云平台API配置（Qwen情报员多平台协同）
BAIDU_QWEN_API_KEY=your-baidu-api-key
TENCENT_QWEN_API_KEY=your-tencent-api-key
VOLCANO_QWEN_API_KEY=your-volcano-api-key
ENABLE_BAIDU_QWEN=true
ENABLE_TENCENT_QWEN=true
ENABLE_VOLCANO_QWEN=true

